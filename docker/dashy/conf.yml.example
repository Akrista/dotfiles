        description: Learning Platform
        icon: si-udemy
        url: https://www.udemy.com/
      - title: Plex
        description: Self-Hosted Media Streaming Service
        icon: si-plex
        url: https://app.plex.tv/desktop
  - name: Utilities
    icon: far fa-hands-helping
    items:
      - title: Dashy
        description: Lissys Dashy
        icon: fas fa-rocket
        url: https://dashy.to
      - title: Public IP
