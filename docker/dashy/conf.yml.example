    items:
      - title: Wiki.js
        description: My Developer Wiki
        icon: si-wikidotjs
        url: https://wiki.node-data-media.de/
      - title: GitLab
        description: Version Sontrol System
        icon: si-gitlab
        url: https://gitlab.com/
      - title: Github
        description: Version Sontrol System
        icon: si-github
        url: https://github.com/
      - title: Stack Overflow
        description: The answer to your problems
        icon: si-stackoverflow
        url: https://stackoverflow.com/
  - name: Virtualisation
    icon: si-docker
    items:
      - title: Portainer
        description: Manage Docker
        icon: si-portainer
        url: http://localhost:9000
      - title: Docker Hub
        description: Docker Hub
        icon: si-docker
        url: https://hub.docker.com
      - title: Kubernetes
        description: Container Orchestrierung
        icon: si-kubernetes
        url: https://kubernetes.io
      - title: Amazon
        description: Amazon Cloud AWS
